#include<stdio.h>
#include <conio.h>


int main() {
    int i,n[5],maior,menor;
    for(i=1; i<=5; ++i) {
        printf("Entre com algum numero:\n");
        scanf("%i",&n[i]);
    

        if(i==1) {
            maior=n[i];
            menor=n[i];
           printf("maior %i\nmenor %i\n",maior,menor);
        }
        else if(n[i]>maior) {
            maior=n[i]; 
            printf("maior %i\nmenor %i\n",maior,menor);
        } else if(n[i]<menor) {
            menor=n[i];
            printf("maior %i\nmenor %i\n",maior,menor);

        }




    }




    return 0;
}