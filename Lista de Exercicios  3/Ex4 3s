#include<stdio.h>
#include <conio.h>
int main()
{

    int n=1;


    printf("Digite um numero ate 50:\n");
    scanf("%i",&n);

    if(n<=50) {
        while(n<250) {


            n=n*3;
            if(n<250)
                printf("%i\n",n);

        }


    }
    else
        printf("Invalido.");

    return 0;
}